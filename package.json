{
  "name": "renature",
  "version": "0.1.0",
  "main": "dist/cjs/renature.js",
  "module": "dist/es/renature.js",
  "scripts": {
    "clean:re": "bsb -clean-world",
    "build:re": "bsb -make-world",
    "start:re": "bsb -make-world -w",
    "build:ts": "tsc",
    "check:ts": "tsc --noEmit",
    "build": "rollup -c rollup.config.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "storybook": "start-storybook -p 6006"
  },
  "keywords": [
    "ReasonML",
    "TypeScript",
    "React",
    "BuckleScript",
    "Animation",
    "Physics"
  ],
  "author": "Parker Ziegler <parker.ziegler@formidable.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@glennsl/bs-jest": "^0.4.9",
    "@rollup/plugin-buble": "^0.21.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@rollup/plugin-typescript": "^2.1.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "bs-platform": "^6.2.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gentype": "^3.6.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/__tests__/**/*_test.bs.js",
      "<rootDir>/__tests__/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(js|ts)x?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@glennsl/bs-jest|bs-platform)/)"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {}
}

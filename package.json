{
  "name": "renature",
  "version": "0.1.0",
  "scripts": {
    "clean:re": "bsb -clean-world",
    "build:re": "bsb -make-world",
    "start:re": "bsb -make-world -w",
    "build:ts": "tsc",
    "check:ts": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "start:example": "parcel example/index.html --out-dir example/build"
  },
  "keywords": [
    "ReasonML",
    "TypeScript",
    "React",
    "BuckleScript",
    "Animation",
    "Physics"
  ],
  "author": "Parker Ziegler <parker.ziegler@formidable.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@glennsl/bs-jest": "^0.4.9",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-jest": "^24.9.0",
    "bs-platform": "^6.2.1",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gentype": "^3.6.0",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/__tests__/**/*_test.bs.js",
      "<rootDir>/__tests__/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(js|ts)x?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@glennsl/bs-jest|bs-platform)/)"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  }
}

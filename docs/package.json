{
  "name": "formidable-renature-docs",
  "version": "5.1.0",
  "description": "Documentation site for renature",
  "main": "static.config.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/renature/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/renature.git"
  },
  "homepage": "https://github.com/FormidableLabs/renature#readme",
  "scripts": {
    "start": "react-static start",
    "clean": "rimraf dist",
    "stage:build": "react-static build --staging",
    "stage:serve": "serve -l 3000 -c serve-staging.json",
    "stage:deploy": "node scripts/deploy/surge.js",
    "prod:build": "react-static build",
    "prod:serve": "serve -l 3000",
    "prod:deploy": "node scripts/deploy/aws.js",
    "prettier-all": "npx prettier --write '{src,static.config,scripts,lib}/**/*.{js,jsx}'",
    "preversion": "yarn run lint && yarn run build",
    "format": "yarn run lint --fix",
    "lint": "eslint 'src/**/*.js' 'static.config.js'",
    "check-ci": "yarn run lint"
  },
  "dependencies": {
    "@mdx-js/react": "^1.5.7",
    "history": "^4.7.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.12.0",
    "react-ga": "^2.4.1",
    "react-live": "^2.2.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-ga": "^1.0.0",
    "react-static": "^7.2.3",
    "react-static-plugin-md-pages": "^0.1.7",
    "renature": "^0.1.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "execa": "^4.0.0",
    "prettier": "^1.15.1",
    "react-hot-loader": "^4.12.20",
    "react-static-plugin-react-router": "^7.2.3",
    "react-static-plugin-sitemap": "^7.2.3",
    "react-static-plugin-styled-components": "^7.2.2",
    "rimraf": "^3.0.0",
    "serve": "^11.3.0",
    "surge": "^0.21.3"
  }
}
